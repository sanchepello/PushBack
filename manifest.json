{
  "manifest_version": 3,
  "name": "CodeKick",
  "version": "1.0",
  "description": "Блокирует выбранные сайты, чтобы не проёбывать жизнь",
  "permissions": ["scripting", "storage", "tabs", "management"],
  "background": {
    "service_worker": "background.js"
  },
"action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
